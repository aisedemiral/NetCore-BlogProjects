@model Mvc_Projem.Controllers.EmployeeTestController.Class1

@{
    ViewData["Title"] = "EditEmployee";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Api İle Personel Düzenleme Sayfası</h1>
<br />
<form method="post">
    <label>ID</label>
    @Html.TextBoxFor(x=>x.ID,new {@class="form-control"})
   <br />
    <label>Personel</label>
    @Html.TextBoxFor(x=>x.Name,new {@class="form-control"})
    <br />
    <button class="btn-outline-success">Güncelle</button>
    </form>
